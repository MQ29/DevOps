@using ApkaNowa.Models
@model List<User>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Imię</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.Name</td>
            </tr>
        }
    </tbody>
</table>
